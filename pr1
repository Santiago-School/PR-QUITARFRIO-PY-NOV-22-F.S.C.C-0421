print("Fco. San. Cor. Car ")
print(" ")

def maxlistas(lista):
    maximo = lista[0]
    for num in lista:
        if num > maximo:
            maximo = num
    print(f"el numero mas grande en la lista {lista} es {maximo}")
    return maximo

# signando valores
numeros = [3, 5, 7, 2, 8, 6]
maxlistas(numeros)

